/etc/grub.d/40_custom 


menuentry ‘Windows′ {  
    set root=’(hd0,msdos2)’  
    chainloader +1  
}

menuentry "Windows 10" --class windows --class os {
   insmod ntfs
   search --no-floppy --set=root --fs-uuid $your_uuid_here$
   ntldr /bootmgr
}

sudo blkid /dev/sda2

sudo update-grub

